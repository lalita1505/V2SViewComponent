@model IEnumerable<DepartmentHeadCount> 

<h3>Employee Head Count Summary</h3>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Department</th>
            <th>Head Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var deptHeadCount in Model)
        {
            <tr>
                <td>
                    @deptHeadCount.DepartmentName
                </td>
                <td>
                    @deptHeadCount.Count
                </td>
            </tr>

        }

    </tbody>
</table>
